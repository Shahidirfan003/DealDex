
<div class="container">
  
  <div class="row">
  <% if(success && success.length>0){ %>
    <div class="alert alert-success alert-dismissible fade show col-md-6 offset-md-3 text-center" role="alert">
      <%= success %>
      <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
      </div>
      <% } %>
    </div>

    <div class="row">
<% if(error && error.length>0){ %>
      <div class="alert alert-danger alert-dismissible fade show col-md-6 offset-md-3 text-center" role="alert">
        <%= error %>
      <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
        </div>
        <% } %>
  </div>
</div>
  
   